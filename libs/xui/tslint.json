{
  "extends": "../../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      ["xui"],
      ["camelCase", "kebab-case"]
    ],
    "component-selector": [
      true,
      ["element", "attribute"],
      ["xui", "test"],
      "kebab-case"
    ]
  },
  "linterOptions": {
    "exclude": ["!**/*"]
  }
}
