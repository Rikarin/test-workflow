{
  "name": "xui",
  "version": "1.6.2",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "nx": "nx",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "pack-scss": "scss-bundle -c libs/xui/scss-bundle.config.json",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "copy-readme": "cp README.md libs/xui/README.md && git add libs/xui/README.md",
    "copy-version": "VERSION=$npm_package_version && cd libs/xui && npm version $VERSION && cd ../.. && git add libs/xui/package.json",
    "release": "git pull origin master && standard-version --sign -a",
    "postrelease": "git push --follow-tags origin master",
    "lol": "npm-run-all copy-*",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.2.11",
    "@angular/cdk": "^12.2.11",
    "@angular/common": "^12.2.11",
    "@angular/compiler": "^12.2.11",
    "@angular/core": "^12.2.11",
    "@angular/forms": "^12.2.11",
    "@angular/material": "^12.2.11",
    "@angular/platform-browser": "^12.2.11",
    "@angular/platform-browser-dynamic": "^12.2.11",
    "@angular/router": "^12.2.11",
    "@nrwl/angular": "13.0.2",
    "ngx-google-analytics": "^11.2.1",
    "ngx-highlightjs": "^5.0.0",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.1",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.11",
    "@angular/cli": "^12.2.11",
    "@angular/compiler-cli": "^12.2.11",
    "@angular/language-service": "^12.2.11",
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@nrwl/cli": "13.0.2",
    "@nrwl/cypress": "13.0.2",
    "@nrwl/jest": "13.0.2",
    "@nrwl/nx-cloud": "12.5.1",
    "@nrwl/workspace": "13.0.2",
    "@types/jest": "27.0.2",
    "@types/node": "~16.11.4",
    "codelyzer": "^6.0.2",
    "cypress": "^8.6.0",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "10.0.0",
    "eslint": "8.1.0",
    "husky": "^7.0.0",
    "jest": "27.3.1",
    "jest-preset-angular": "10.0.1",
    "ng-packagr": "^12.2.4",
    "npm-run-all": "^4.1.5",
    "postcss-import": "^14.0.2",
    "prettier": "2.4.1",
    "pretty-quick": "^3.1.1",
    "scss-bundle": "^3.1.2",
    "standard-version": "^9.3.2",
    "ts-jest": "27.0.7",
    "ts-node": "~10.4.0",
    "tslint": "~6.1.3",
    "typescript": "~4.3.5"
  },
  "standard-version": {
    "scripts": {
      "postbump": "npm-run-all copy-*"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
