{
  "name": "xui",
  "version": "1.8.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "nx": "nx",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "pack-scss": "scss-bundle -c libs/xui/scss-bundle.config.json",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "copy-readme": "cp README.md libs/xui/README.md && git add libs/xui/README.md",
    "copy-version": "VERSION=$npm_package_version && cd libs/xui && npm version $VERSION && cd ../.. && git add libs/xui/package.json",
    "release": "git pull origin master && standard-version -a",
    "postrelease": "git push --follow-tags origin master",
    "prepare": "husky install",
    "format:all": "nx format:write --all"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^13.3.3",
    "@angular/cdk": "^13.3.3",
    "@angular/common": "^13.3.3",
    "@angular/compiler": "^13.3.3",
    "@angular/core": "^13.3.3",
    "@angular/forms": "^13.3.3",
    "@angular/material": "^13.3.3",
    "@angular/platform-browser": "^13.3.3",
    "@angular/platform-browser-dynamic": "^13.3.3",
    "@angular/router": "^13.3.3",
    "@mdi/angular-material": "^6.6.96",
    "@ngneat/until-destroy": "^9.0.2",
    "@nrwl/angular": "13.10.3",
    "@oddbird/blend": "^0.2.3",
    "ngx-google-analytics": "^13.0.1",
    "ngx-highlightjs": "^6.1.2",
    "rxjs": "~7.5.5",
    "tslib": "^2.3.1",
    "zone.js": "^0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.3.3",
    "@angular/cli": "^13.3.3",
    "@angular/compiler-cli": "^13.3.3",
    "@angular/language-service": "^13.3.3",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@nrwl/cli": "13.10.3",
    "@nrwl/cypress": "13.10.3",
    "@nrwl/jest": "13.10.3",
    "@nrwl/nx-cloud": "13.3.1",
    "@nrwl/workspace": "13.10.3",
    "@types/jest": "27.4.1",
    "@types/node": "~17.0.25",
    "codelyzer": "^6.0.2",
    "cypress": "^9.5.4",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "16.0.0",
    "eslint": "8.13.0",
    "husky": "^7.0.0",
    "jest": "27.5.1",
    "jest-preset-angular": "11.1.1",
    "ng-packagr": "^13.3.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.12",
    "postcss-import": "^14.1.0",
    "postcss-url": "^10.1.3",
    "prettier": "2.6.2",
    "pretty-quick": "^3.1.3",
    "scss-bundle": "^3.1.2",
    "standard-version": "^9.3.2",
    "ts-jest": "27.1.4",
    "ts-node": "~10.7.0",
    "tslint": "~6.1.3",
    "typescript": "~4.6.3"
  },
  "standard-version": {
    "scripts": {
      "postbump": "npm-run-all copy-*"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
